{
	"name": "tunimillion-back",
	"version": "1.0.0",
	"description": "tunimillion backend project",
	"author": "Dundill",
	"keywords": [
		"nestjs",
		"bootstrap",
		"express",
		"graphql",
		"apollo-graphql",
		"typeorm"
	],
	"scripts": {
		"build": "tsc -p tsconfig.build.json",
		"format": "prettier --write \"src/**/*.ts\"",
		"all-tests": "npm run test && npm run test:e2e",
		"start": "ts-node -r tsconfig-paths/register src/main.ts",
		"start:dev": "nodemon",
		"start:debug": "nodemon --config nodemon-debug.json",
		"prestart:prod": "rm -rf dist && tsc",
		"start:prod": "NODE_ENV=production node -r ./tsconfig-paths-bootstrap.js dist/main.js",
		"lint": "tslint -p tsconfig.json -c tslint.json",
		"lint:fix": "tslint --fix -p tsconfig.json -c tslint.json",
		"test": "jest -w 1 --config ./jest.config.json --forceExit",
		"test:watch": "NODE_ENV=testing jest -w 1 --watch --forceExit",
		"test:cov": "NODE_ENV=testing jest -w 1 --config ./jest.config.json --coverage --forceExit",
		"test:debug": "NODE_ENV=testing node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest -w 1 --runInBand --forceExit",
		"test:e2e": "jest -w 1 --config ./test/jest-e2e.json --forceExit",
		"webpack": "webpack --config webpack.config.js",
		"start:hmr": "node dist/main.js",
		"gen": "ts-node src/generator/generate-typings.ts",
		"db:seed": "ts-node src/dbseed.ts",
		"doc": "npx compodoc --port 11045 -p tsconfig.json -s",
		"doc:serve": "yarn doc && npx compodoc -s",
		"heroku-postbuild": "yarn --only=dev --no-shrinkwrap && yarn build && yarn gen",
		"now-build": "yarn --only=dev --no-shrinkwrap && yarn build && yarn gen",
		"dump": "ts-node src/dump",
		"coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls"
	},
	"dependencies": {
		"@apollo/federation": "0.20.4",
		"@apollo/gateway": "0.21.1",
		"@nestjs/axios": "^2.0.0",
		"@nestjs/bull": "^0.3.0",
		"@nestjs/common": "7.4.4",
		"@nestjs/core": "7.4.4",
		"@nestjs/graphql": "^7.9.1",
		"@nestjs/microservices": "^8.0.8",
		"@nestjs/platform-express": "7.4.4",
		"@nestjs/schedule": "^0.4.3",
		"@nestjs/typeorm": "^7.1.5",
		"amqp-connection-manager": "^3.0.0",
		"amqplib": "^0.6.0",
		"apollo-server": "^2.19.0",
		"apollo-server-cache-memcached": "^0.6.3",
		"apollo-server-express": "^2.9.7",
		"apollo-server-plugin-base": "^3.7.2",
		"apollo-server-plugin-response-cache": "^0.5.6",
		"assert": "^2.0.0",
		"axios": "^0.21.4",
		"bcrypt": "^5.0.0",
		"bull": "^3.13.0",
		"cache-manager": "^3.4.0",
		"chai": "^4.2.0",
		"class-transformer": "^0.3.1",
		"class-validator": "^0.12.2",
		"cloudinary": "^1.23.0",
		"compression": "^1.7.4",
		"cron": "^1.7.2",
		"csurf": "^1.10.0",
		"dateformat": "^4.3.1",
		"dotenv": "^8.2.0",
		"express-rate-limit": "^5.0.0",
		"flatted": "^3.2.7",
		"fs": "0.0.2",
		"generate-password": "^1.6.0",
		"google-translate": "^3.0.0",
		"graphql": "^15.4.0",
		"graphql-depth-limit": "^1.1.0",
		"graphql-rate-limit": "^2.0.1",
		"graphql-subscriptions": "^2.0.0",
		"graphql-tools": "^7.0.2",
		"graphql-type-json": "^0.3.0",
		"graphql-voyager": "^1.0.0-rc.28",
		"handlebars": "^4.4.5",
		"helmet": "^4.2.0",
		"ioredis": "^4.14.1",
		"jest-config": "^26.6.3",
		"jsonwebtoken": "^8.5.1",
		"merge-graphql-schemas": "^1.7.0",
		"mongodb": "^3.3.3",
		"morgan": "^1.9.1",
		"multer": "^1.4.2",
		"nodemailer": "^6.3.1",
		"nodemailer-express-handlebars": "^4.0.0",
		"passport": "^0.4.0",
		"passport-facebook": "^3.0.0",
