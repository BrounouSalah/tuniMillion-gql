		"subscriptions-transport-ws": "^0.11.0",
		"tsconfig-paths": "^3.9.0",
		"typeorm": "^0.2.20",
		"typescript": "^4.1.2",
		"uuid": "^8.3.1",
		"webpack-bundle-analyzer": "^4.2.0",
		"winston": "^3.2.1"
	},
	"devDependencies": {
		"@compodoc/compodoc": "^1.1.11",
		"@nestjs/testing": "^7.5.5",
		"@types/bull": "^3.12.1",
		"@types/cron": "^2.0.1",
		"@types/express": "^4.17.1",
		"@types/jest": "^26.0.24",
		"@types/mocha": "^10.0.1",
		"@types/mongodb": "^3.3.6",
		"@types/node": "^14.14.10",
		"@types/nodemailer": "^6.2.1",
		"@types/stripe": "^8.0.417",
		"@types/supertest": "^2.0.8",
		"codecov": "^3.6.1",
		"concurrently": "^5.0.0",
		"coveralls": "^3.0.7",
		"husky": "^4.3.5",
		"jest": "^26.6.3",
		"lint-staged": "^10.5.3",
		"nodemon": "^2.0.6",
		"prettier": "^2.2.1",
		"progress-bar-webpack-plugin": "^2.1.0",
		"supertest": "^6.0.1",
		"ts-jest": "^26.4.4",
		"ts-loader": "^8.0.11",
		"ts-node": "^10.8.1",
		"tslint": "^6.1.3",
		"tslint-clean-code": "^0.2.10",
		"tslint-config-prettier": "^1.18.0",
		"wait-on": "^5.2.0",
		"webpack": "^5.10.0",
		"webpack-cli": "^4.2.0",
		"webpack-loader-helper": "^1.0.0",
		"webpack-node-externals": "^2.5.2",
		"yarn": "^1.19.1"
	},
	"lint-staged": {
		"*.ts": [
			"yarn format",
			"yarn lint:fix",
			"git add"
		]
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	}
}
